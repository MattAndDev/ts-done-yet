{
  "name": "ts-done-yet",
  "version": "0.1.1",
  "description": "how far is your migration to typescript?",
  "main": "build/index.js",
  "types": "source/main.ts",
  "bin": {
    "ts-done-yet": "bin/index.js"
  },
  "scripts": {
    "dev": "nodemon --watch 'source/**/*.ts' --exec 'ts-node' source/index.ts",
    "start": "tsc && node ./build/index.js",
    "prepublishOnly": "tsc"
  },
  "keywords": [],
  "devDependencies": {
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "standard": "^16.0.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "author": {
    "name": "Matteo Maronati",
    "email": "mail@matteo.land",
    "url": "https://matteo.land"
  },
  "license": "WTFPL",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "url": "https://github.com/MattAndDev/ts-done-yet"
  },
  "lint-staged": {
    "**/*.ts": [
      "prettier --write",
      "eslint ./source --ext .ts --fix"
    ]
  }
}
